{
    "name": "samlitowitz/github-copilot-crud-test",
    "description": "CRUD app to test github copilot",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Litowitz",
            "email": "samlitowitz@users.noreply.github.com"
        }
    ],
    "minimum-stability": "stable",
    "sort-packages": true,
    "config": {
        "platform": {
            "php": "8.1"
        }
    },
    "bin": [
        "bin/crud-app"
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "phpstan/phpstan": "^2.1",
        "samlitowitz/phpstan-opinionated-nomenclature": "^1.0",
        "squizlabs/php_codesniffer": "^3.12",
        "phpstan/phpstan-symfony": "^2.0"
    }
}
